{
	"name": "server",
	"version": "1.0.0",
	"description": "Backend server for the remaster",
	"main": "./dist/index.js",
	"type": "module",
	"scripts": {
		"watch": "tsc -w",
		"docker:up": "docker-compose -f docker-compose.yml up -d",
		"dev": "nodemon ./dist/index.js",
		"test": "dotenv -e .env.test -- jest --config jest.config.js --detectOpenHandles",
		"docker:up:test": "docker compose -f docker-compose-test.yml up -d && dotenv -e .env.test -- npx prisma db push && npm run test && docker compose -f docker-compose-test.yml down",
		"docker:down:test": "docker compose -f docker-compose-test.yml down",
		"studio": "npx prisma studio",
		"studio:test": "dotenv -e .env.test -- nodemon ./dist/index.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/josenavasiv/remaster-server.git"
	},
	"author": "Jose Navas",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/josenavasiv/remaster-server/issues"
	},
	"homepage": "https://github.com/josenavasiv/remaster-server#readme",
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/connect-redis": "^0.0.20",
		"@types/cookie": "^0.5.1",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/formidable": "^2.0.5",
		"@types/jest": "^29.4.0",
		"@types/node": "^18.14.6",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^9.0.1",
		"@types/validator": "^13.7.13",
		"@types/ws": "^8.5.4",
		"jest": "^29.5.0",
		"nodemon": "^2.0.21",
		"prisma": "^4.11.0",
		"supertest": "^6.3.3",
		"ts-jest": "^29.0.5",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@apollo/server": "^4.4.1",
		"@aws-sdk/client-s3": "^3.296.0",
		"@graphql-tools/schema": "^9.0.17",
		"@prisma/client": "^4.11.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.20.2",
		"connect-redis": "^7.0.0",
		"cookie": "^0.5.0",
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"express-session": "^1.17.3",
		"formidable": "^3.2.5",
		"graphql": "^16.6.0",
		"graphql-redis-subscriptions": "^2.6.0",
		"graphql-subscriptions": "^2.0.0",
		"graphql-ws": "^5.12.0",
		"ioredis": "^5.3.1",
		"nanoid": "^4.0.1",
		"uuid": "^9.0.0",
		"validator": "^13.9.0",
		"ws": "^8.13.0"
	}
}
